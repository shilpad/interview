#include <map>
#include <iostream>
using namespace std;

/* Problem : G(1,2,3,4).
 *
 */

int main ()
{
	int arr [] = {2,3,4};
	int len = sizeof (arr)/sizeof(arr[0]);
	int total = 0;
	int times = len - 1;

	for (int i=0; i < len; ++i)
	{
		total += arr[i] * times;
	}

	cout << "Total is " << total << endl;
}



